    "localforage": "^1.7.3",
    "lodash": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "minio": "^7.0.8",
    "muse-ui": "^3.0.2",
    "muse-ui-loading": "^0.2.0",
    "muse-ui-message": "^0.2.1",
    "muse-ui-progress": "^0.1.0",
    "muse-ui-toast": "^0.3.0",
    "normalize.css": "^8.0.1",
    "readable-blob-stream": "^1.1.0",
