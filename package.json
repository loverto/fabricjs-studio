{
  "name": "fabric-stduio",
  "version": "0.1.7",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:prod": "yarn build:fonts && vue-cli-service build",
    "build:fonts": "node build/build-fonts.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
